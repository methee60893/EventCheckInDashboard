<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EventCheckInDashboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.js"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/dashboard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EventCheckInDashboard.styles.css" asp-append-version="true" />
</head>
<body>
    <!-- Navigation Bar -->
    <div class="navbar-container">
        <nav class="navbar navbar-expand-lg custom-navbar">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">DASHBOARD KPCOB JOYFINITY CELEBRATION</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item">
                            @* <a id="a_main" class="nav-link" href="/Index">Overview</a> *@
                            <a id="a_main" class="nav-link" href="/dashboardevent/Index">Overview</a>
                        </li>
                        <li class="nav-item">
                            @* <a id="a_inst" class="nav-link" href="/InStoreTroop">IN-STORE TROOP</a> *@
                            <a id="a_stpw" class="nav-link" href="/dashboardevent/InStoreTroop">IN-STORE TROOP</a>
                        </li>
                        <li class="nav-item">
                            @* <a id="a_aird" class="nav-link" href="/AirDrop">AIR DROP</a> *@
                            <a id="a_tpwr" class="nav-link" href="/dashboardevent/AirDrop">AIR DROP</a> 
                        </li>
                        <li class="nav-item">
                            @* <a id="a_diybag" class="nav-link" href="/DIYBagKnitting">DIY BAG KNITTING</a> *@
                            <a id="a_drtpw" class="nav-link" href="/dashboardevent/DIYBagKnitting">DIY BAG KNITTING</a> 
                        </li>
                        <li class="nav-item">
                            @* <a id="a_candle" class="nav-link" href="/CandleStack">CANDLE STACK</a> *@
                            <a id="a_tpwps" class="nav-link" href="/dashboardevent/CandleStack">CANDLE STACK</a> 
                        </li>
                        <li class="nav-item">
                            @* <a id="a_diykey" class="nav-link" href="/DIYKeyChainKnitting">DIY KEY CHAIN KNITTING</a> *@
                            <a id="a_exapo" class="nav-link" href="/dashboardevent/DIYKeyChainKnitting">DIY KEY CHAIN KNITTING</a> 
                        </li>
                        <li class="nav-item">
                            @* <a id="a_matcha" class="nav-link" href="/MatchaBarLover">MATCHA BAR LOVER</a> *@
                            <a id="a_exapo" class="nav-link" href="/dashboardevent/MatchaBarLover">MATCHA BAR LOVER</a>
                        </li>
                        <li class="nav-item">
                            @* <a id="a_matcha" class="nav-link" href="/SnapShare">SNAP, SHARE & ENJOY</a> *@
                            <a id="a_exapo" class="nav-link" href="/dashboardevent/SnapShare">SNAP, SHARE & ENJOY</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    <div class="dashboard-container">
       
            @RenderBody()

    </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>